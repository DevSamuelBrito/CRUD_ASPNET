{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=app.db"
  },
  "Swagger": {
    "RoutePrefix": "api/docs",
    "JsonRouteTemplate": "api/docs/{documentName}/swagger.json"
  },
  "AllowedHosts": "*"
}
